<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="470.0" prefWidth="491.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mmt.comradepigman.gui.ACreateProject">
   <bottom>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="6.0" prefWidth="531.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" onAction="#saveProject" text="Save" />
         </children>
      </HBox>
   </bottom>
   <top>
      <VBox prefHeight="22.0" prefWidth="531.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="26.0" prefWidth="433.0" styleClass="h1" text="%ACreateProject.PROJECT_SETTINGS" />
         </children>
      </VBox>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label styleClass="form-label" text="%ACreateProject.PROJECT_TITLE" />
            <TextField fx:id="projectTitle" />
            <Label styleClass="form-label" text="%ACreateProject.FOLDER_NAME" />
            <TextField fx:id="projectFolder" />
            <Label styleClass="form-label" text="%ACreateProject.SUPPORTED_VERSION" />
            <TextField />
            <Label styleClass="form-label" text="%ACreateProject.PROJECT_TAGS" />
            <Label styleClass="h2" text="%ACreateProject.PROJECT_SETTINGS" />
            <Label styleClass="form-label" text="%ACreateProject.DIR_PATH" />
            <HBox prefHeight="16.0" prefWidth="600.0" styleClass="dir-selector">
               <children>
                  <TextField HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" styleClass="btn" text="Select" />
               </children>
            </HBox>
            <Label styleClass="form-label" text="%ACreateProject.PROJECT_TAGS" />
            <HBox prefHeight="16.0" prefWidth="600.0" styleClass="dir-selector">
               <children>
                  <TextField HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" styleClass="btn" text="Select" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
